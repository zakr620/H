				{Goal = { ImageTransparency = 0 }, Duration = 0.5}
			})
		end,
		Click = function()
			setclipboard(Options.YoutubeLink)
			Window:Notify({
				Title = "Link Copied!",
				Subtitle = "The youtube link has been copied\n To your clipboard.",
				Duration = 6
			})
		end,
	})

